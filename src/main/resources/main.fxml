<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            onKeyPressed="#onDeletePressed" prefHeight="458.0" prefWidth="767.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.sergei.komarov.java.patterns.lab4.mvc.controllers.Controller">
    <children>
        <LineChart fx:id="chart" layoutX="246.0" layoutY="32.0">
            <xAxis>
                <NumberAxis side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT"/>
            </yAxis>
        </LineChart>
        <TableView fx:id="valuesTable" editable="true" layoutX="26.0" layoutY="44.0" prefHeight="364.0"
                   prefWidth="217.0"/>
        <TextField layoutX="26.0" layoutY="420.0" prefHeight="25.0" prefWidth="80.0" promptText="x"
                   fx:id="xInputField"/>
        <TextField fx:id="yInputField" layoutX="113.0" layoutY="420.0" prefHeight="25.0" prefWidth="80.0"
                   promptText="f(x)"/>
        <Button layoutX="200.0" layoutY="420.0" mnemonicParsing="false" onAction="#onAddButtonClick" prefHeight="25.0"
                prefWidth="43.0" text="+"/>
    </children>
</AnchorPane>
